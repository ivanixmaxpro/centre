<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="FrontBundle\Entity\Pacient" table="pacient">
    <id name="dni" type="string" column="dni" length="25">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="25" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cognom" type="string" column="cognom" length="25" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dolencia" type="string" column="dolencia" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
